{
  "name": "coffee-shop",
  "version": "1.0.0",
  "description": "Coffee Shop Backend APIs",
  "main": "src/index.ts",
  "scripts": {
    "start": "node index.js",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "mocha": "mocha --require ts-node/register src/test/*.test.ts",
    "migrate": "npx prisma migrate dev",
    "clean": "rm -rf www/ || true",
    "build": "npm install . && npm run clean && tsc && cp -rf build/ www/ && cp -R .elasticbeanstalk www/.elasticbeanstalk && cp .npmrc www/.npmrc && cp package.json www/package.json && cd www && zip -r Archive.zip . && cd ..",
    "deploy": "npm run build && eb list && eb use coffee-shop && eb deploy && chmod +x bin/eb_setenv.sh && bin/eb_setenv.sh",
    "test": "npm run mocha"
  },
  "author": "Saif Abuelhasan",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.1.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.0.0",
    "prisma": "^4.4.0",
    "supertest": "^6.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.5",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
